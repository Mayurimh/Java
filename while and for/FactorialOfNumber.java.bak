import java.util.Scanner;
class FactorialOfNumber
{
	public static void main(String [] args){
		Scanner ip = new Scanner(System.in);
		System.out.println("Enter a number  : ");
		int num = ip.nextInt();
		int dup = num
		
		int fact = 1;
		for(int i=1; i<=num; i++){
			fact *= i;
		}
		System.out.println("Fcatorial of number "+dup+" is "+fact );
	}
}