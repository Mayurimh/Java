import java.util.Scanner;
class Season
{
	public static void main(String [] args){
		Scanner ip = new Scanner(System.in);
		System.out.println();
		System.out.println("     WELCOME IN SEASON CHECK APPLICATION ");
		System.out.println();
		System.out.println("Enter Month name : ");
		String month = ip.next().toLowerCase();
		switch(month){
			case "november":
			case "december":
			case "january":
			case "February":
			{
				System.out.println("Noses are red Fingers are blue..Because,");
				System.out.println("it is Winter..");
				break;
			}
			
			case "march":
			case "april":
			case "may":
			case "june":
			{
				System.out.println("Stay hydrated and wear sunscreen. Because,");
				System.out.println("it is Summer..");
				break;
			}
			
			case "july":
			case "august":
			case "september":
			{
				System.out.println("Drink clean and boiled Water & keep umbrella. Because,");
				System.out.println("it is Rainy..");
				break;
			}
			case "october":{
				System.out.println("October is autumn season..");
				break;
			}
			default :
			{
				System.out.println("Invalid Month :(");
			}
		}
	}
}